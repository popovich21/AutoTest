			"age":"18"
			}
		},
		"response":{
			"text":"这是一个带参数的get响应"
		}

	},
	{
		"description":"模拟一个带参数的get请求",
		"request":{
			"uri":"/temp",
			"method":"get"			
		},
		"response":{
			"text":{
			"template":"${req.queries['name']}:${req.queries['age']}"
			}
		}

	}
