[
	{
		"description":"模拟一个没有参数的get请求",
		"request":{
			"uri":"/getdemo",
			"method":"get"
		},
		"response":{
			"text":"这是一个没有参数的get响应"
		}

	},
	{
		"description":"模拟一个带参数的get请求",
		"request":{
			"uri":"/getdemo1",
